
@model PrimerProyecto.Models.Pago

@{
    ViewData["Title"] = "Pagar";
}

<h1>Pagar</h1>
<div>
    <hr />

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NroPago)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NroPago)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaPago)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaPago)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Importe)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Importe)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ContratoId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContratoId)
        </dd>
    </dl>
    <form asp-action="Pagar">
        <input type="submit" value="Pagar" class="btn btn-warning" /> |
        <a asp-action="Index" class="btn btn-info">Regresar a la lista</a>
    </form>
</div>
